{
  "expo": {
    "name": "WatchBase",
    "slug": "watchbase",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/images/wb-icon.png",
    "scheme": "watchbase",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/wb-splash.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.watchbase.app"
    },
    "android": {
      "package": "com.watchbase.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wb-logo-trns.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/wb-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a3c3db1f-1479-4d33-847a-216aba516bbf"
      }
    }
  }
}
